{
  "target": "bmv2",
  "p4info": "build/main.p4.p4info.txt",
  "bmv2_json": "build/main.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dst_addr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dst_addr": "08:00:00:00:01:11",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dst_addr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dst_addr": "08:00:00:00:02:22",
        "port": 2
      }
    },
    {
      "table": "MyIngress.sw_id",
      "default_action": true,
      "action_name": "MyIngress.set_sw_id",
      "action_params": { 
        "sw_id": 1
      }
    }
  ]
}
